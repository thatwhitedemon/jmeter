              <stringProp name="script">import org.jsoup.Jsoup
import java.util.Random
import java.time.LocalDate
import java.time.format.DateTimeFormatter

// Получаем HTML-ответ от предыдущего запроса
String response = prev.getResponseDataAsString()

// Проверяем, что ответ не пустой
if (response != null &amp;&amp; !response.trim().isEmpty()) {

    // Разбираем HTML с помощью Jsoup
    def doc = Jsoup.parse(response)

    // Создаём списки для хранения городов &quot;Откуда&quot; и &quot;Куда&quot;
    def departureCities = []
    def destinationCities = []

    // Находим список городов &quot;Откуда&quot; (из &lt;select name=&quot;depart&quot;&gt;)
    def optionsDepart = doc.select(&quot;select[name=depart] option&quot;)
    
    // Используем стандартный for вместо each
    for (int i = 0; i &lt; optionsDepart.size(); i++) {
        def option = optionsDepart.get(i)  // Получаем &lt;option&gt;
        String city = option.attr(&quot;value&quot;).trim()  // Берём значение &quot;value&quot;
        if (!city.isEmpty()) {  // Проверяем, что город не пустой
            departureCities.add(city)  // Добавляем в список
        }
    }

    // Находим список городов &quot;Куда&quot; (из &lt;select name=&quot;arrive&quot;&gt;)
    def optionsArrive = doc.select(&quot;select[name=arrive] option&quot;)

    // Используем for вместо each
    for (int i = 0; i &lt; optionsArrive.size(); i++) {
        def option = optionsArrive.get(i)  // Получаем &lt;option&gt;
        String city = option.attr(&quot;value&quot;).trim()  // Берём значение &quot;value&quot;
        if (!city.isEmpty()) {  // Проверяем, что город не пустой
            destinationCities.add(city)  // Добавляем в список
        }
    }

    // Сохраняем списки городов в JMeter-переменные
   vars.put(&quot;departureCities_matchNr&quot;, departureCities.size().toString())
   vars.put(&quot;destinationCities_matchNr&quot;, destinationCities.size().toString())
   //log.info(&quot;DEBUG: departureCities_matchNr = &quot; + vars.get(&quot;departureCities_matchNr&quot;))
	//log.info(&quot;DEBUG: destinationCities_matchNr = &quot; + vars.get(&quot;destinationCities_matchNr&quot;))

    for (int i = 0; i &lt; departureCities.size(); i++) {
        vars.put(&quot;departureCities_&quot; + (i + 1), departureCities.get(i))
    }

    for (int i = 0; i &lt; destinationCities.size(); i++) {
        vars.put(&quot;destinationCities_&quot; + (i + 1), destinationCities.get(i))
    }

// Проверяем, что список &quot;Откуда&quot; не пуст
if (!departureCities.isEmpty()) {
    Random random = new Random()
    // Выбираем случайный город &quot;Откуда&quot;
    String selectedDeparture = departureCities.get(random.nextInt(departureCities.size()))
    vars.put(&quot;selectedDeparture&quot;, selectedDeparture)
} else {
    vars.put(&quot;selectedDeparture&quot;, &quot;NOT_FOUND&quot;)
}

// Проверяем, что список &quot;Куда&quot; не пуст
if (!destinationCities.isEmpty()) {
    Random random = new Random()
    String selectedDestination = &quot;&quot;

    // Проверяем, что есть хотя бы 2 города, иначе выбирать другой нет смысла
    if (destinationCities.size() &gt; 1) {
        // Генерируем случайный город &quot;Куда&quot;, пока он совпадает с &quot;Откуда&quot;
        do {
            selectedDestination = destinationCities.get(random.nextInt(destinationCities.size()))
        } while (selectedDestination.equals(vars.get(&quot;selectedDeparture&quot;)))
    } else {
        selectedDestination = destinationCities.get(0)  // Единственный возможный город
    }

    // Сохраняем город &quot;Куда&quot;
    vars.put(&quot;selectedDestination&quot;, selectedDestination)
} else {
    vars.put(&quot;selectedDestination&quot;, &quot;NOT_FOUND&quot;)
}

} else {
    vars.put(&quot;selectedDeparture&quot;, &quot;NO_RESPONSE&quot;)
    vars.put(&quot;selectedDestination&quot;, &quot;NO_RESPONSE&quot;)
}
		// вычисление даты 
// Получаем текущую дату
LocalDate today = LocalDate.now()

// Вычисляем даты
LocalDate departDate = today.plusDays(1)  // Завтра
LocalDate returnDate = today.plusDays(2)  // Послезавтра

// Форматируем даты в формат MM/dd/yyyy
DateTimeFormatter formatter = DateTimeFormatter.ofPattern(&quot;MM/dd/yyyy&quot;)
String formattedDepartDate = departDate.format(formatter)
String formattedReturnDate = returnDate.format(formatter)

// Сохраняем даты в JMeter-переменные
vars.put(&quot;departDate&quot;, formattedDepartDate)
vars.put(&quot;returnDate&quot;, formattedReturnDate)</stringProp>
