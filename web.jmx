              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">import org.apache.jmeter.protocol.http.sampler.HTTPSamplerBase

// Получаем HTTP-запрос
def sampler = ctx.getCurrentSampler() as HTTPSamplerBase

// Очищаем старые параметры (если они есть)
sampler.getArguments().clear()
sampler.addArgument(&quot;removeAllFlights.x&quot;,&quot;49&quot;)
sampler.addArgument(&quot;removeAllFlights.y&quot;,&quot;9&quot;)
// Получаем количество билетов
int count = vars.get(&quot;flightID_matchNr&quot;) as int

// Добавляем все flightID в параметры
for (int i = 1; i &lt;= count; i++) {
    sampler.addArgument(&quot;flightID&quot;, vars.get(&quot;flightID_&quot; + i))
}

// Добавляем все .cgifields в параметры
for (int i = 1; i &lt;= count; i++) {
    sampler.addArgument(&quot;.cgifields&quot;, vars.get(&quot;cgiField_&quot; + i))
}


</stringProp>
